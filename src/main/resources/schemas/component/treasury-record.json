{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "type": {
      "pattern": "^[A-Z]$",
      "type": "string"
    },
    "idElements": {
      "pattern": "^[A-Z0-9]{17}$",
      "type": "string"
    },
    "tributeCode": {
      "pattern": "^[A-Z0-9]{4}$",
      "type": "string"
    },
    "reportingYear": {
      "pattern": "^[1-2][0-9]{3}$",
      "type": "string"
    },
    "debitAmount": {
      "pattern": "^0|[1-9][0-9]{0,14}$",
      "type": "string"
    }
  },
  "description": "Treasury Record object"
}