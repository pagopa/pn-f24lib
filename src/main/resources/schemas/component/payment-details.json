{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "required": [
    "dateOfPayment"
  ],
  "type": "object",
  "properties": {
    "dateOfPayment": {
      "pattern": "^\\d{2}-\\d{2}-\\d{4}$",
      "type": "string"
    },
    "company": {
      "type": "string"
    },
    "cabCode": {
      "type": "string"
    },
    "checkNumber": {
      "type": "string"
    },
    "abiCode": {
      "type": "string"
    },
    "ibanCode": {
      "type": "string"
    },
    "bank": {
      "type": "boolean"
    }
  },
  "description": "Payment Details object"
}