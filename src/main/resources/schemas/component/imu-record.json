{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "municipalityCode": {
      "pattern": "^[0-9A-Z]{4}$",
      "type": "string"
    },
    "repentance": {
      "type": "boolean"
    },
    "changedBuildings": {
      "type": "boolean"
    },
    "advancePayment": {
      "type": "boolean"
    },
    "payment": {
      "type": "boolean"
    },
    "numberOfBuildings": {
      "pattern": "^[0-9]{3}$",
      "type": "string"
    },
    "tributeCode": {
      "pattern": "^[0-9]{4}$",
      "type": "string"
    },
    "installment": {
      "pattern": "^[A-Z0-9]{0,4}$",
      "type": "string"
    },
    "year": {
      "pattern": "^[1-2][0-9]{3}$",
      "type": "string"
    },
    "debitAmount": {
      "pattern": "^0|[1-9][0-9]{0,14}$",
      "type": "string"
    },
    "creditAmount": {
      "pattern": "^0|[1-9][0-9]{0,14}$",
      "type": "string"
    }
  },
  "description": "IMU Record object"
}