{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "municipality": {
      "pattern": "^[A-Z]{2}$",
      "type": "string"
    },
    "province": {
      "pattern": "^[A-Z]{2}$",
      "type": "string"
    },
    "idCode": {
      "pattern": "^[A-Z0-9]{17}$",
      "type": "string"
    },
    "taxTypeCode": {
      "pattern": "^[0-9A-Z]{4}$",
      "type": "string"
    },
    "installment": {
      "pattern": "^[A-Z0-9]{0,4}$",
      "type": "string"
    },
    "month": {
      "pattern": "^[0-1][0-9]$",
      "type": "string"
    },
    "year": {
      "pattern": "^[1-2][0-9]{3}$",
      "type": "string"
    },
    "debitAmount": {
      "pattern": "^[0-9]{0,15}$",
      "default": "0",
      "type": ["string", "null"]
    }
  },
  "description": "Excise Tax object"
}